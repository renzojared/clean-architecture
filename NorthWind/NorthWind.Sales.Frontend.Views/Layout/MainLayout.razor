@inherits NavMenuLayout

<SectionContent
    SectionId="NavBarBrandSection">
    <a class="navbar-brand" href="">
        @CommonMessages.NavBarBrandTitle
    </a>
</SectionContent>

<SectionContent
    SectionId="NavBarItemsSection">
    <NavMenuItem Href="" Match="NavLinkMatch.All">
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-house-door-fill" viewBox="0 0 16 16">
