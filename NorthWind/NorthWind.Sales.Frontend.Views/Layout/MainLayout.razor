        </svg>
        @CreateOrderMessages.CreateOrderMenuItemTitle
    </NavMenuItem>
</SectionContent>

<SectionContent
    SectionId="HeaderSection">
    <a href="https://learn.microsoft.com/aspnet/core/" target="_blank">
        @CommonMessages.AboutTitle
    </a>
</SectionContent>

@((RenderFragment)(builder => base.BuildRenderTree(builder)))